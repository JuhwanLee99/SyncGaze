# Core
GOOGLE_CLOUD_PROJECT=your-gcp-project
REGION=us-central1
SERVICE_NAME=syncgaze-ingestor
SUMMARY_TARGETS=firestore,bigquery

# Storage trigger
FIREBASE_STORAGE_BUCKET=your-bucket-name

# Firestore document paths
FS_SUMMARY_COLLECTION=sessionSummaries
FS_PROFILE_PATH=users/{uid}
FS_SURVEY_PATH=sessions/{sessionId}/survey
FS_CONSENT_PATH=sessions/{sessionId}/consent
FS_CALIBRATION_PATH=sessions/{sessionId}/calibration

# BigQuery
BIGQUERY_DATASET=syncgaze
BIGQUERY_TABLE=joinedData

# Authentication (choose one)
FIREBASE_SERVICE_ACCOUNT_JSON={"type": "service_account", ...}
# or rely on GOOGLE_APPLICATION_CREDENTIALS / Workload Identity
